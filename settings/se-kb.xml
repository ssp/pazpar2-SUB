<?xml version="1.0" encoding="UTF-8"?>
<settings target="z3950.libris.kb.se/*" xmlns="http://www.indexdata.com/pazpar2/1.0">
<!--
	pazpar2 settings for the catalogues of Sweden’s national library,
	created for vifanord.
	
	The database name is »libris«.
	The Index 5001 can be used to limit the search to specific sub-databases.
	Sub-databses are listed on the page:
		http://libris.kb.se/deldatabas.jsp?more=hum
	
	2014: Sven-S. Porst <ssp-web@earthlingsoft.net>
-->

	<!-- Search -->
	<set name="pz:cclmap:term" value="u=1016 t=l,r s=al"/>

	<set name="pz:cclmap:title" value="u=4 s=al 2=102"/>
	<set name="pz:cclmap:person" value="u=1001 s=al"/>
	
	<set name="pz:cclmap:journal" value="u=5 s=al"/>
	<set name="pz:cclmap:subject" value="u=21 s=al"/>
	<set name="pz:cclmap:isbn" value="u=7"/>
	<set name="pz:cclmap:issn" value="u=8"/>

	<set name="pz:cclmap:date" value="u=31"/>
	<set name="pz:cclmap:ppn" value="u=5000"/>
	<set name="pz:cclmap:collection" value="u=5001"/>


	<set name="pz:limitmap:subject" value="ccl:subject"/>
	<set name="pz:limitmap:collection" value="ccl:collection"/>


	<!-- Retrieval -->
	<set name="pz:requestsyntax" value="marc21"/>
	<set name="pz:elements" value="F"/>
	<set name="pz:queryencoding" value="utf-8"/>
	<!-- Server does not support piggybacking -->
	<set name="pz:piggyback" value="0"/>


	<!-- Normalisation -->
	<set name="pz:nativesyntax" value="txml;utf-8"/>
	<set name="pz:xslt" value="xsl/target/se-kb-pre.xsl,xsl/convert/tmarc.xsl,xsl/pz/final/finalise.xsl"/>


	<set name="region" value="nord"/>

	<set name="catalogueURLHintPrefix" target="z3950.libris.kb.se/*" value="http://libris.kb.se/bib/"/>

	<set target="z3950.libris.kb.se/libris" name="pz:name" value="KB Schweden"/>
	<set target="z3950.libris.kb.se/libris" name="pz:pqf_prefix" value="
		@and
		@or @or @or @or @or @or @or @or @or @or @or
		@or @or @or @or @or @or @or @or @or @or @or
		@attr 1=5001 NB
		@attr 1=5001 COL
		@attr 1=5001 SB17
		@attr 1=5001 SOT
		@attr 1=5001 SHB
		@attr 1=5001 SLB
		@attr 1=5001 BIRB
		@attr 1=5001 BULB
		@attr 1=5001 KVIN
		@attr 1=5001 SAMB
		@attr 1=5001 SWAM
		@attr 1=5001 NLT
		@attr 1=5001 LOB
		@attr 1=5001 DALA
		@attr 1=5001 GOTL
		@attr 1=5001 OSGO
		@attr 1=5001 SAH
		@attr 1=5001 SARB
		@attr 1=5001 SFBB
		@attr 1=5001 SNUB
		@attr 1=5001 VSTM
		@attr 1=5001 VIMO
		@attr 1=5001 LITT
	"/>
	
	
