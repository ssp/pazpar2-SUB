<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../../xsl/doc/pazpar2-docs.xsl"?>
<service
	xmlns="http://www.indexdata.com/pazpar2/1.0"
	xmlns:xi="http://www.w3.org/2001/XInclude"
	id="vifanord">
<!--
	Targets for vifanord main search.
	http://vifanord.de

	2014-2015: Sven-S. Porst <ssp-web@earthlingsoft.net>
-->

	<settings src="settings/vifanord-general.xml"/>
	
	<settings src="settings/gbv-sru.xml"/>
	<settings src="settings/vifanord-gbv.xml"/>
	<settings>
		<set target="sru.gbv.de/fachopac-vifanord-nord" name="pz:allow" value="1"/>
		<set target="sru.gbv.de/olc-vifanord-nord" name="pz:allow" value="1"/>
	</settings>
	
	<settings src="settings/dk-kb.xml"/>
	<settings>
		<set target="dk-kb/KGL01_BDANICA" name="pz:allow" value="1"/>
		<set target="dk-kb/BGF01_DHB" name="pz:allow" value="1"/>
		<set target="dk-kb/BGF01_DSB" name="pz:allow" value="1"/>
		<set target="dk-kb/MUS01" name="pz:allow" value="1"/>
		<set target="dk-kb/KGL01_DAPF" name="pz:allow" value="1"/>
	</settings>
	
	<settings src="settings/se-kb.xml"/>
	<settings>
		<set target="se-kb/libris-vifanord" name="pz:allow" value="1"/>
	</settings>
	
	<settings src="settings/no-bibsys.xml"/>
	<settings src="settings/fi-helka.xml"/>
	<settings src="settings/bonis.xml"/>
	
	<settings src="settings/hu-berlin.xml"/>
	<settings>
		<set target="hu-berlin/TB_SKANDINAVISTIK" name="pz:allow" value="1"/>
	</settings>
	
	<settings src="settings/swb.xml"/>
	<settings src="settings/vifanord-swb.xml"/>
	<settings>
		<set target="swb/wao-vifanord-nord" name="pz:allow" value="1"/>
	</settings>
	
	<settings src="settings/fes.xml"/>



	<xi:include href="../metadata/vifanord-extras.xml"/>
	<xi:include href="../metadata/default.xml"/>

</service>
